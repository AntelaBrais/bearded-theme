{
  "$schema": "vscode://schemas/color-theme",
  "name": "BeardedTheme Light",
  "colors": {
    // main bg: ffffff
    // main bg lighten: ebecee
    // main bg lighten lighten: caced4 // borders
    // primary: 8544ff
    // primary dark: c7a8ff
    // green: 4bb46e
    // green apple: 99c24e
    // yellow: e09914
    // blue: 36a3ec
    // pink: cb55e2
    // salmon: e4567c
    // purple: af6bfd
    // orange: ff7728
    // turquoize: 4fb99a
    // red: ff4949
    // punctuation: 00000071

    // UI -------------------------------------------------
    // input
    "input.background": "#0000000c",
    "input.foreground": "#000000b4",
    "input.placeholderForeground": "#aaa",
    "inputOption.activeBorder": "#000000b0",
    "inputValidation.errorBackground": "#c7a8ff",
    "inputValidation.errorBorder": "#e09914",
    "inputValidation.infoBackground": "#c7a8ff",
    "inputValidation.infoBorder": "#8544ff",
    "inputValidation.warningBackground": "#c7a8ff",
    "inputValidation.warningBorder": "#e09914",
    // progressBar
    "progressBar.background": "#e09914",
    // scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#0000003f",
    "scrollbarSlider.background": "#00000018",
    "scrollbarSlider.hoverBackground": "#00000027",
    // badge
    "badge.background": "#8544ff93",
    "badge.foreground": "#000000",
    // button
    "button.background": "#8544ff",
    "button.foreground": "#000000",
    // debug
    "debugExceptionWidget.background": "#c7a8ff",
    "debugExceptionWidget.border": "#aaa",
    "debugToolBar.background": "#c7a8ff",
    // description
    "descriptionForeground": "#aaa",
    // dropdown (ex: select in terminal)
    "dropdown.background": "#ffffff",
    "dropdown.border": "#caced4",
    "dropdown.foreground": "#0000007c",
    // extensionButton
    "extensionButton.prominentBackground": "#8544ff9d",
    "extensionButton.prominentForeground": "#000000",
    "extensionButton.prominentHoverBackground": "#8544ff",
    "focusBorder": "#fdfdfd42",
    "foreground": "#aaa",

    // ACTIVITY BAR ------------------------------------
    "activityBar.background": "#ebecee",
    "activityBar.foreground": "#000000a4",
    "activityBar.border": "#caced4",
    "activityBarBadge.background": "#8544ff93",
    "activityBarBadge.foreground": "#000000",

    // EDITOR -------------------------------------------
    "editor.background": "#ffffff",
    "editor.foreground": "#000000",
    "editor.findMatchBackground": "#FF720066",
    "editor.findMatchHighlightBackground": "#CAD40F66",
    "editor.hoverHighlightBackground": "#8644ff3d",
    "editor.inactiveSelectionBackground": "#003b8b",
    "editor.lineHighlightBackground": "#8544ff0c",
    "editor.lineHighlightBorder": "#8544ff1a",
    "editor.rangeHighlightBackground": "#8544ff21",
    // Bread
    "breadcrumbPicker.background": "#ffffff",
    // selection click
    "editor.selectionBackground": "#8544ff38",
    "editor.selectionHighlightBorder": "#00000041",
    "editor.wordHighlightStrongBackground": "#00000021",
    "editor.wordHighlightBackground": "#00000021",
    "editorBracketMatch.background": "#8544ff5d",
    "editorBracketMatch.border": "#8544ff96",
    "editorCodeLens.foreground": "#aaa",
    "editorCursor.foreground": "#e09914",
    // gutter
    "editorGutter.background": "#ffffff",
    "editorGutter.addedBackground": "#4bb46e",
    "editorGutter.deletedBackground": "#ff4949",
    "editorGutter.modifiedBackground": "#36a3ec",
    // editorGroup
    "editorGroup.border": "#caced4",
    "editorGroup.dropBackground": "#8544ff15",
    // editorGroupHeader
    "editorGroupHeader.noTabsBackground": "#ebecee",
    "editorGroupHeader.tabsBackground": "#ebecee",
    "editorGroupHeader.tabsBorder": "#ebecee",
    // editorHoverWidget
    "editorHoverWidget.background": "#ffffff",
    "editorHoverWidget.border": "#0000002f",
    "editorIndentGuide.background": "#0000000e",
    "editorLineNumber.foreground": "#0000003a",
    "editorLink.activeForeground": "#aaa",
    // editorMarkerNavigation
    "editorMarkerNavigation.background": "#3B536433",
    "editorMarkerNavigationError.background": "#A22929",
    "editorMarkerNavigationWarning.background": "#e09914",
    // ruler
    "editorOverviewRuler.border": "#00000000",
    "editorOverviewRuler.commonContentForeground": "#e0991455",
    "editorOverviewRuler.currentContentForeground": "#ee3a4355",
    "editorOverviewRuler.incomingContentForeground": "#3ad90055",
    // editorSuggestWidget
    "editorSuggestWidget.background": "#caced4",
    "editorSuggestWidget.border": "#0000002c",
    "editorSuggestWidget.foreground": "#000000b0",
    "editorSuggestWidget.highlightForeground": "#e09914",
    "editorSuggestWidget.selectedBackground": "#8544ff4d",
    // editorWarning (ex: search ctrl f)
    "editorWarning.border": "#00000000",
    "editorWarning.foreground": "#e09914",
    "editorWhitespace.foreground": "#c7a8ff",
    "editorWidget.background": "#c7a8ff",
    "errorForeground": "#A22929",
    // diff
    "diffEditor.insertedTextBackground": "#4bb46e33",
    "diffEditor.insertedTextBorder": "#4bb46e55",
    "diffEditor.removedTextBackground": "#ee3a4333",
    "diffEditor.removedTextBorder": "#ee3a4355",

    // SIDEBAR
    "sideBar.background": "#ebecee",
    "sideBar.border": "#ffffff",
    "sideBar.foreground": "#000000ab",
    "sideBarSectionHeader.background": "#caced49f",
    "sideBarSectionHeader.foreground": "#fafafabb",
    "sideBarTitle.foreground": "#777777",
    // List
    "list.activeSelectionBackground": "#8544ff38",
    "list.dropBackground": "#8544ff15",
    "list.focusBackground": "#8544ff38",
    "list.focusForeground": "#000000",
    "list.highlightForeground": "#e09914",
    "list.hoverBackground": "#0000000a",
    "list.hoverForeground": "#000000",
    "list.inactiveSelectionBackground": "#8544ff38",
    // Git status colors in File Explorer
    "gitDecoration.modifiedResourceForeground": "#e09914",
    "gitDecoration.deletedResourceForeground": "#af6bfd",
    "gitDecoration.untrackedResourceForeground": "#4bb46e",
    "gitDecoration.ignoredResourceForeground": "#0000003a",
    "gitDecoration.conflictingResourceForeground": "#e4567c",
    // END SIDEBAR

    // menu
    "menu.background": "#122738",
    // merge
    "merge.border": "#00000000",
    "merge.commonContentBackground": "#c97d0c",
    "merge.commonHeaderBackground": "#c97d0c",
    "merge.currentContentBackground": "#2F7366",
    "merge.currentHeaderBackground": "#2F7366",
    "merge.incomingContentBackground": "#185294",
    "merge.incomingHeaderBackground": "#185294",
    // notification colors - The colors below only apply for VS Code versions 1.21 and higher.
    "notificationCenterHeader.foreground": "#aaa",
    "notificationCenterHeader.background": "#c7a8ff",
    "notifications.foreground": "#aaa",
    "notifications.background": "#c7a8ff",
    "notificationLink.foreground": "#e09914",
    // panel
    "panel.background": "#ffffff",
    "panel.border": "#caced4",
    "panelTitle.activeBorder": "#8544ff",
    "panelTitle.activeForeground": "#8544ff",
    "panelTitle.inactiveForeground": "#00000054",
    // "peekView
    "peekView.border": "#e09914",
    "peekViewEditor.background": "#c7a8ff",
    "peekViewEditor.matchHighlightBackground": "#c7a8ff00",
    "peekViewEditorGutter.background": "#122738",
    "peekViewResult.background": "#15232d",
    "peekViewResult.fileForeground": "#aaa",
    "peekViewResult.lineForeground": "#000000",
    "peekViewResult.matchHighlightBackground": "#8544ff",
    "peekViewResult.selectionBackground": "#8544ff",
    "peekViewResult.selectionForeground": "#000000",
    "peekViewTitle.background": "#15232d",
    "peekViewTitleDescription.foreground": "#aaa",
    "peekViewTitleLabel.foreground": "#e09914",
    // picker
    "pickerGroup.border": "#caced4",
    "pickerGroup.foreground": "#aaa",

    // selection
    "selection.background": "#027dff",

    // STATUS BAR ----------------------------------------------------
    "statusBar.background": "#caced4",
    "statusBar.border": "#caced4",
    "statusBar.debuggingBackground": "#8544ff44",
    "statusBar.debuggingBorder": "#8544ff",
    "statusBar.debuggingForeground": "#000000af",
    "statusBar.foreground": "#616161",
    "statusBar.noFolderBackground": "#15232d",
    "statusBar.noFolderBorder": "#8544ff",
    "statusBar.noFolderForeground": "#aaa",
    "statusBarItem.activeBackground": "#0088ff",
    "statusBarItem.hoverBackground": "#c7a8ff",
    "statusBarItem.prominentBackground": "#15232d",
    "statusBarItem.prominentHoverBackground": "#c7a8ff",

    // TABS -------------------------------------------------
    "tab.activeBackground": "#8644ff44",
    "tab.activeForeground": "#8544ff",
    "tab.border": "#caced4",
    "tab.activeBorder": "#8544ff",
    "tab.inactiveBackground": "#ebecee",
    "tab.inactiveForeground": "#0000004b",
    "tab.unfocusedActiveForeground": "#aaa",
    "tab.unfocusedInactiveForeground": "#aaa",

    // TERMINAL -------------------------------------------
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#ff628c",
    "terminal.ansiGreen": "#4bb46e",
    "terminal.ansiYellow": "#e09914",
    "terminal.ansiBlue": "#36a3ec",
    "terminal.ansiMagenta": "#cb55e2",
    "terminal.ansiCyan": "#2fc6c9",
    "terminal.ansiWhite": "#000000",
    "terminal.ansiBrightBlack": "#0050A4",
    "terminal.ansiBrightRed": "#ff628c",
    "terminal.ansiBrightGreen": "#4bb46e",
    "terminal.ansiBrightYellow": "#e09914",
    "terminal.ansiBrightBlue": "#36a3ec",
    "terminal.ansiBrightMagenta": "#cb55e2",
    "terminal.ansiBrightCyan": "#2fc6c9",
    "terminal.ansiBrightWhite": "#c7a8ff",
    "terminal.background": "#ffffff",
    "terminal.foreground": "#e9e9e9",
    "terminalCursor.background": "#e09914",
    "terminalCursor.foreground": "#e09914",

    // textBlockQuote
    "textBlockQuote.background": "#36a3ec34",
    "textBlockQuote.border": "#36a3ecb9",
    "textCodeBlock.background": "#36a3ec34",
    "textLink.activeForeground": "#36a3ec",
    "textLink.foreground": "#36a3ec",
    "textPreformat.foreground": "#e09914",
    "textSeparator.foreground": "#8544ff",
    "titleBar.activeBackground": "#15232D",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#c7a8ff",
    "titleBar.inactiveForeground": "#00000033",
    "walkThrough.embeddedEditorBackground": "#00000025",
    "widget.shadow": "#07070700"
  },
  "tokenColors": [
    {
      "name": "Current text",
      "scope": [
        "source.python",
        "source.perl",
        "source.ruby",
        "source.shell",
        "source.sql",
        "source.go"
      ],
      "settings": {
        "foreground": "#000000b6"
      }
    },
    {
      "name": "Entity / functions",
      "scope": [
        "source.js entity",
        "source.js support.function",
        "source.php entity",
        "source.php support.function"
      ],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "Storage Type Function",
      "scope": [
        "source.js storage.type.function",
        "source.php storage.type.function",
        "source.java storage"
      ],
      "settings": {
        "foreground": "#af6bfd",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0000003d"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f44542"
      }
    },
    {
      "name": "Storage Type Function",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#ff9d00"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, storage.type.class",
      "settings": {
        "foreground": "#ff9d00"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "Meta JSX",
      "scope": [
        "meta.jsx.children",
        "meta.jsx.children.js",
        "meta.jsx.children.tsx"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Meta Brace",
      "scope": ["meta.brace", "source.js meta"],
      "settings": {
        "foreground": "#000000cc"
      }
    },
    {
      "name": "Punctuation default text",
      "scope": [
        "punctuation",
        "meta.brace",
        "source.js punctuation.definition.parameters",
        "source.php punctuation.definition.parameters",
        "source.python punctuation.definition.parameters",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#00000056"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#ffee80"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "source.js string.template",
        "source.js punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#4bb46e"
      }
    },
    {
      "name": "String Template",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#3ad900"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#ff7728"
      }
    },
    {
      "name": "Support Function",
      "scope": "support.function",
      "settings": {
        "foreground": "#ff9d00"
      }
    },
    {
      "name": "Support Variable Property DOM",
      "scope": "support.variable.property.dom",
      "settings": {
        "foreground": "#000000cc"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "punctuation.definition.variable.less",
        "punctuation.definition.variable.php",
        "variable.language",
        "entity.name.type.class.js"
      ],
      "settings": {
        "foreground": "#e4567c"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#ff4949"
      }
    },
    {
      "name": "[CSS] - pseudo",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#ff4949"
      }
    },
    {
      "name": "[CSS] - Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix.css punctuation"
      ],
      "settings": {
        "foreground": "#4bb46e"
      }
    },
    {
      "name": "[CSS] - &",
      "scope": [
        "entity.name.tag.reference.scss",
        "keyword.control.at-rule.media.css punctuation",
        "keyword.control.at-rule.import.scss",
        "entity.other.attribute-name.parent-selector.css punctuation"
      ],
      "settings": {
        "foreground": "#cc4f3f"
      }
    },
    {
      "name": "[CSS] - ID Selector",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[CSS] - Element Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#af6bfd"
      }
    },
    {
      "name": "[CSS] - Support",
      "scope": [
        "source.css support",
        "source.css entity.name.tag",
        "meta.property-list.css",
        "meta.property-list.scss"
      ],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[CSS] - Constant",
      "scope": [
        "source.css constant",
        "source.css support.constant",
        "constant.numeric.css keyword"
      ],
      "settings": {
        "foreground": "#000000b6"
      }
    },

    {
      "name": "[CSS] - String",
      "scope": [
        "source.css string",
        "source.css punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#4bb46e"
      }
    },
    {
      "name": "[CSS] - Variable",
      "scope": ["source.css variable", "source.stylus variable"],
      "settings": {
        "foreground": "#e4567c"
      }
    },
    {
      "name": "[HTML] - Text",
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#000000b6"
      }
    },
    {
      "name": "[HTML] - Attribute",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.localname.xml"
      ],
      "settings": {
        "foreground": "#e09914",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[HTML] - Entity Name",
      "scope": [
        "text.html.basic entity.name",
        "entity.name.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.xml"
      ],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[HTML] - ID value",
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#A5FF90"
      }
    },
    {
      "name": "[HTML] - Entity Other",
      "scope": "text.html.basic entity.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e09914"
      }
    },
    {
      "name": "[HTML] - Script Tag",
      "scope": "meta.tag.metadata.script.html entity.name.tag.html",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[HTML] - Quotes. these are a slightly different colour because expand selection will then not include quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#4bb46e"
      }
    },
    {
      "name": "[INI] - Entity",
      "scope": "source.ini entity",
      "settings": {
        "foreground": "#000000cc"
      }
    },
    {
      "name": "[INI] - Keyword",
      "scope": "source.ini keyword",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[INI] - Punctuation Definition",
      "scope": "source.ini punctuation.definition",
      "settings": {
        "foreground": "#ffee80"
      }
    },
    {
      "name": "[INI] - Punctuation Separator",
      "scope": "source.ini punctuation.separator",
      "settings": {
        "foreground": "#ff9d00"
      }
    },
    {
      "name": "[JAVASCRIPT] - Inherited Component",
      "scope": "entity.other.inherited-class.js",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "name": "[PYTHON]",
      "scope": ["storage.type.class", "storage.type.function"],
      "settings": {
        "foreground": "#4fb99a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[PYTHON] - Self Argument",
      "scope": "variable.parameter.function.language.special.self.python",
      "settings": {
        "foreground": "#cb55e2"
      }
    },
    {
      "name": "[JSON] - Support",
      "scope": [
        "source.json support",
        "punctuation.support.type.property-name.begin.json.comments punctuation"
      ],
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[MARKDOWN] - Heading Punctuation",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#e4567c"
      }
    },
    {
      "name": "[MARKDOWN] - List",
      "scope": "markup.list.unnumbered.markdown meta.paragraph.markdown",
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Raw",
      "scope": [
        "markup.inline.raw.string.markdown",
        "punctuation.definition.raw.markdown",
        "markup.fenced_code.block.markdown",
        "markup.fenced_code.block.markdown punctuation"
      ],
      "settings": {
        "foreground": "#af6bfd"
      }
    },
    {
      "name": "[MARKDOWN] - Heading Name Section",
      "scope": [
        "entity.name.section.markdown",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown"
      ],
      "settings": {
        "foreground": "#e09914",
        "fontStyle": "bold"
      }
    },
    {
      "name": "[MARKDOWN] - Paragraph",
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#000000cc"
      }
    },
    {
      "name": "[MARKDOWN] - Quote Punctuation",
      "scope": "beginning.punctuation.definition.quote.markdown",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[MARKDOWN] - Quote Paragraph",
      "scope": "markup.quote.markdown meta.paragraph.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[MARKDOWN] - Emphasis Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Emphasis Italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Lists",
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[MARKDOWN] - Link Title",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#a5ff90"
      }
    },
    {
      "name": "[MARKDOWN] - Link/Image Title",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown",
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#a5ff90"
      }
    },
    {
      "name": "[MARKDOWN] - Link Address",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Inline Code",
      "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[MARKDOWN] - Code Block",
      "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[TYPESCRIPT] - Entity Name Type",
      "scope": "source.ts entity.name.type",
      "settings": {
        "foreground": "#4fb99a"
      }
    },
    {
      "name": "[TYPESCRIPT] - Keyword",
      "scope": "source.ts keyword",
      "settings": {
        "foreground": "#e09914"
      }
    },
    {
      "name": "[TYPESCRIPT] - Punctuation Parameters",
      "scope": "source.ts punctuation.definition.parameters",
      "settings": {
        "foreground": "#000000cc"
      }
    },
    {
      "name": "[TYPESCRIPT] - Punctuation Arrow Parameters",
      "scope": "meta.arrow.ts punctuation.definition.parameters",
      "settings": {
        "foreground": "#ffee80"
      }
    },
    {
      "name": "[TYPESCRIPT] - Storage",
      "scope": "source.ts storage",
      "settings": {
        "foreground": "#36a3ec"
      }
    },
    {
      "name": "[TYPESCRIPT] - Inherited Component",
      "scope": "entity.other.inherited-class.ts, entity.other.inherited-class.tsx",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "name": "Italicsify for Operator Mono",
      "scope": [
        "modifier",
        "this",
        "comment",
        "storage.modifier.js",
        "storage.modifier.ts",
        "storage.modifier.tsx",
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.ts",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      // Elm Types
      "name": "[ELM] - Types",
      "scope": [
        "meta.function.type-declaration.elm storage",
        "storage.type.elm"
      ],
      "settings": {
        "foreground": "#af6bfd"
      }
    },
    {
      "name": "[ELM] - Accessor",
      "scope": ["record.accessor.elm", "entity.name.record.field.accessor.elm"],
      "settings": {
        "foreground": "#ff4949"
      }
    },
    {
      "name": "[ELM] - Import",
      "scope": ["keyword.control.import.elm", "keyword.other.elm"],
      "settings": {
        "foreground": "#99c24e"
      }
    },
    {
      // Elm Type Constructor
      "name": "[ELM] - Type Constructor",
      "scope": ["source.elm constant"],
      "settings": {
        "foreground": "#e4567c"
      }
    },
    {
      // Elm Imports
      "name": "[ELM] - Imports",
      "scope": ["meta.module.name.elm support", "meta.import.elm support"],
      "settings": {
        "foreground": "#ff7728"
      }
    },
    {
      // Elm Imports
      "name": "[ELM] - Meta",
      "scope": ["source.elm meta"],
      "settings": {
        "foreground": "#000000b6"
      }
    },
    {
      // Elm Imports
      "name": "[ELM] - String",
      "scope": [
        "punctuation.definition.string.begin.elm",
        "punctuation.definition.string.end.elm",
        "source.elm string"
      ],
      "settings": {
        "foreground": "#4bb46e"
      }
    },
    {
      // Elm Imports
      "name": "[ELM] - Punctuation",
      "scope": ["source.elm punctuation"],
      "settings": {
        "foreground": "#00000071"
      }
    },
    {
      // Elm Imports
      "name": "[ELM] - Function",
      "scope": [
        "meta.function.type-declaration.elm entity",
        "entity.name.function.top_level.elm"
      ],
      "settings": {
        "foreground": "#36a3ec"
      }
    }
  ]
}
